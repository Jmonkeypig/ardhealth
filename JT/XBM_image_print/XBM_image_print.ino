void setup(void) {
  
  u8g2.begin();  
  u8g2.setFont(u8g2_font_ncenB18_tr);   // choose a suitable font
  Wire.begin();      //Wire 라이브러리 초기화
  Wire.beginTransmission(MPU); //MPU로 데이터 전송 시작
  Wire.write(0x6B);  // PWR_MGMT_1 register
  Wire.write(0);     //MPU-6050 시작 모드로
  Wire.endTransmission(true);
  
  BTSerial.begin(9600);
  Serial.begin(9600);

  
}
void writeString(String stringData) { // Used to serially push out a String with Serial.write()

  for (int i = 0; i < stringData.length(); i++)
  {
    BTSerial.write(stringData[i]);   // Push each char 1 by 1 on each loop pass
  }

}// end writeString

void loop(void) {
  if (system_status == SYSTEM_STATUS_READY) {
    
    draw_smile();

    if(BTSerial.available()){
      int inChar = BTSerial.read();
       
      if (isDigit(inChar)) {
        // convert the incoming byte to a char and add it to the string:
        inString = (char)inChar;
        Serial.println(inString);
        if(inString == '0'){
          Serial.println("count");
          tone(speakerpin,500,500);  //500: 음의 높낮이(주파수), 1000: 음의 지속시간(1초)
          system_status = SYSTEM_STATUS_COUNT;
        }
      }
      
    }
    delay(100);
  }
  if (system_status == SYSTEM_STATUS_COUNT) {
    Serial.println("3");
    count = 3;
//    u8g2.clearBuffer();                    // clear the internal memory
//u8g2.firstPage();
    draw_dummbel();
    tone(speakerpin,501,500);  //500: 음의 높낮이(주파수), 1000: 음의 지속시간(1초)
    delay(1000);
    Serial.println("2");
    count = 2;
    tone(speakerpin,502,500);  //500: 음의 높낮이(주파수), 1000: 음의 지속시간(1초)
    delay(1000);
    count = 1;
    Serial.println("1");
    tone(speakerpin,503,500);  //500: 음의 높낮이(주파수), 1000: 음의 지속시간(1초)
    delay(1000);
    count = 0;
    system_status = SYSTEM_STATUS_PLAYING;
  } else if (system_status == SYSTEM_STATUS_PLAYING) {
    Wire.beginTransmission(MPU);    //데이터 전송시작
    Wire.write(0x3B);               // register 0x3B (ACCEL_XOUT_H), 큐에 데이터 기록
    Wire.endTransmission(false);    //연결유지
      
    Wire.requestFrom(MPU,6,true);  //MPU에 데이터 요청
    
  
    //데이터 한 바이트 씩 읽어서 반환
    AcX = Wire.read() << 8 | Wire.read();  // 0x3B (ACCEL_XOUT_H) & 0x3C (ACCEL_XOUT_L)    
    AcY = Wire.read() << 8 | Wire.read();  // 0x3D (ACCEL_YOUT_H) & 0x3E (ACCEL_YOUT_L)
    AcZ = Wire.read() << 8 | Wire.read();  // 0x3F (ACCEL_ZOUT_H) & 0x40 (ACCEL_ZOUT_L)
    //Tmp = Wire.read() << 8 | Wire.read();  // 0x41 (TEMP_OUT_H) & 0x42 (TEMP_OUT_L)
    //GyX = Wire.read() << 8 | Wire.read();  // 0x43 (GYRO_XOUT_H) & 0x44 (GYRO_XOUT_L)
    //GyY = Wire.read() << 8 | Wire.read();  // 0x45 (GYRO_YOUT_H) & 0x46 (GYRO_YOUT_L)
    //GyZ = Wire.read() << 8 | Wire.read();  // 0x47 (GYRO_ZOUT_H) & 0x48 (GYRO_ZOUT_L)
  
  
    float RADIAN_TO_DEGREES = 180/3.14139;
    float val_y = atan(AcX/sqrt(pow(AcY,2) + pow(AcZ,2))) * RADIAN_TO_DEGREES;
//    float val_x = atan(AcY/sqrt(pow(AcX,2) + pow(AcZ,2))) * RADIAN_TO_DEGREES;
  
    //시리얼 모니터에 출력
    //Serial.print("AcX = "); 
//    Serial.println(val_y);
  //Serial.print(" | AcY = "); Serial.println(val_x);
//  Serial.print(" | AcZ = "); Serial.println(AcZ);
  //Serial.print(" | Tmp = "); Serial.print(Tmp/340.00+36.53);  
//  Serial.print(" | GyX = "); Serial.println(GyX);
  //Serial.print(" | GyY = "); Serial.print(GyY);
//  Serial.print(" | GyZ = "); Serial.println(GyZ);
  
    if(val_y < 0){
      if(checkCount == true){
          checkCount = false;
          
          count = count +1;
          drawFloat(count);
//          char ShowData = count;
//          BTSerial.write(ShowData);
          writeString(String(count));
      }
    }

     if(val_y > 80){
      if(checkCount == false){
          checkCount = true;
          tone(speakerpin,505,500);  //500: 음의 높낮이(주파수), 1000: 음의 지속시간(1초)
          if(count ==10){
            count =0;
            
            tone(speakerpin,500,500);  //500: 음의 높낮이(주파수), 1000: 음의 지속시간(1초)
            draw_smile();
            
          delay(200);
          }
      }
    }

     if(BTSerial.available()){

//      Serial.write(BTSerial.read());
        int inChar = BTSerial.read();
       
